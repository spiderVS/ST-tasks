{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "sourceMap": false,
    "outDir": "dist",
    "rootDir": "src",
    "strict": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noImplicitAny": true,
    "typeRoots": [
      "./@types",
      "./node_modules/@types"
    ]
  },
  "exclude":[
    "./node_modules"
  ]
}
